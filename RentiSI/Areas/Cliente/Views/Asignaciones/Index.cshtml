@{
    ViewData["Title"] = "Asignacion";
}


<div class="mt-5">
    <div class="row">
        <div class="col-2 offset-5">
            <h3>
                <small class="text-body-secondary">Asignaciones</small>
            </h3>
        </div>
    </div>
</div>
<div class="">

    <br />
    <div class="row">
        <div class="col-4 offset-4">
            <a asp-action="Create" class="btn btn-primary"><i class="fa-solid fa-plus"></i> Crear Nueva Asignación</a>
        </div>
    </div>
    <br />
    <table id="tblAsignaciones" class="table table-striped table-bordered" style="width: 100%">
        <thead>
            <tr>
                <th style="text-align: center;">Id</th>
                <th style="text-align: center;">Fecha Creación</th>
                <th style="text-align: center;">Número Placa</th>
                <th style="text-align: center;">Financiación</th>
                <th style="text-align: center;">Impronta</th>
                <th style="text-align: center;">Fecha Negocio</th>
                <th style="text-align: center;">Organismo Tránsito</th>
                <th style="text-align: center;">Observaciones</th>
                <th style="text-align: center;">Acciones</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/asignacion.js"></script>
}